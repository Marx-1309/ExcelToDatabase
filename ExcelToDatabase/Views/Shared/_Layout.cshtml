<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - HrGp-Integration </title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" href="//cdn.datatables.net/2.0.2/css/dataTables.dataTables.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link href="~/sweetalert/sweetalert.css" rel="stylesheet" />
    <link href="~/toast-notification/toastmin.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/ExcelToDatabase.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-dark bg-black border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index"> <img src="~/orc_icon.jfif" style="height:30px" /> Omusati Regional Council</a>
            </div>

        </nav>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                    <partial name="_LoginPartialCustom" />
                    <ul class="navbar-nav flex-grow-1">
                        
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" data-bs-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false"><img src="~/bsicons/upload.svg" /> Upload Files</a>
                            <div class="dropdown-menu">
                                <a class="dropdown-item" asp-controller="Pay_UploadInstance" asp-action="Index"> Earnings & Deductions </a>
                               @*  <a class="dropdown-item" asp-controller="Home" asp-action="UploadExcel">Deductions</a> *@
                            </div>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" data-bs-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false"><img src="~/bsicons/tools.svg" /> Tools</a>
                            <div class="dropdown-menu">
                                <a class="nav-link text-dark" asp-area="" asp-controller="Pay_Earning" asp-action="Index">Earnings</a>
                                <a class="nav-link text-dark" asp-area="" asp-controller="Pay_PayPoint" asp-action="Index">Pay Point</a>
                                @*  <a class="dropdown-item" asp-controller="Home" asp-action="UploadExcel">Deductions</a> *@
                            </div>
                        </li>

                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Pay_Account" asp-action="Index"><img src="~/bsicons/person-vcard.svg" /> GL Accounts config</a>
                        </li>
                       
                        
                    </ul>
                   
                   
                </div>
                
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
            <partial name="_Notifications" />
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            <a onclick="window.open('https://kineticwizard.com.na/','_blank','height=400,width=400')">&copy; @DateTime.UtcNow.Year.ToString() - Kinetic Wizard Software Solutions - All rights reserved. -</a>
             <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="//cdn.datatables.net/2.0.2/js/dataTables.min.js"></script>
    <script src="~/sweetalert/sweetalermin.js"></script>
    <script src="~/datetimepicker/dist/date-time-picker.min.js"></script>
    <script src="~/toast-notification/toastrmin.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>

